{
  "include": ["index.ts"],
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./dist",
    "strict": true,
    "lib": ["ES2022", "ES2015.Collection"],
    "target": "es2022"
  }
}
